#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/louisegilligan/GitRepos/guess-the-character/server/node_modules/.pnpm/ipx@2.0.1_@netlify+blobs@6.3.1/node_modules/ipx/bin/node_modules:/Users/louisegilligan/GitRepos/guess-the-character/server/node_modules/.pnpm/ipx@2.0.1_@netlify+blobs@6.3.1/node_modules/ipx/node_modules:/Users/louisegilligan/GitRepos/guess-the-character/server/node_modules/.pnpm/ipx@2.0.1_@netlify+blobs@6.3.1/node_modules:/Users/louisegilligan/GitRepos/guess-the-character/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/louisegilligan/GitRepos/guess-the-character/server/node_modules/.pnpm/ipx@2.0.1_@netlify+blobs@6.3.1/node_modules/ipx/bin/node_modules:/Users/louisegilligan/GitRepos/guess-the-character/server/node_modules/.pnpm/ipx@2.0.1_@netlify+blobs@6.3.1/node_modules/ipx/node_modules:/Users/louisegilligan/GitRepos/guess-the-character/server/node_modules/.pnpm/ipx@2.0.1_@netlify+blobs@6.3.1/node_modules:/Users/louisegilligan/GitRepos/guess-the-character/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/ipx.mjs" "$@"
else
  exec node  "$basedir/../../bin/ipx.mjs" "$@"
fi
