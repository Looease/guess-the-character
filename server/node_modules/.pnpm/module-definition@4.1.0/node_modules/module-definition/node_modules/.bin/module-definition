#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/louisegilligan/GitRepos/guess-the-character/server/node_modules/.pnpm/module-definition@4.1.0/node_modules/module-definition/bin/node_modules:/Users/louisegilligan/GitRepos/guess-the-character/server/node_modules/.pnpm/module-definition@4.1.0/node_modules/module-definition/node_modules:/Users/louisegilligan/GitRepos/guess-the-character/server/node_modules/.pnpm/module-definition@4.1.0/node_modules:/Users/louisegilligan/GitRepos/guess-the-character/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/louisegilligan/GitRepos/guess-the-character/server/node_modules/.pnpm/module-definition@4.1.0/node_modules/module-definition/bin/node_modules:/Users/louisegilligan/GitRepos/guess-the-character/server/node_modules/.pnpm/module-definition@4.1.0/node_modules/module-definition/node_modules:/Users/louisegilligan/GitRepos/guess-the-character/server/node_modules/.pnpm/module-definition@4.1.0/node_modules:/Users/louisegilligan/GitRepos/guess-the-character/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
